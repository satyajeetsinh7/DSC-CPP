stack<int> ss; // auxiliary stack for minimums

void push(stack<int>& s, int a) {
    s.push(a);

    if (ss.empty() || a <= ss.top()) {
        ss.push(a);
    }
}

bool isFull(stack<int>& s, int n) {
    return s.size() == n;
}

bool isEmpty(stack<int>& s) {
    return s.empty();
}

int pop(stack<int>& s) {
    if (s.empty()) return -1;

    int x = s.top();
    s.pop();

    if (!ss.empty() && ss.top() == x) {
        ss.pop();
    }

    return x;
}

int getMin(stack<int>& s) {
    if (ss.empty()) return -1;
    return ss.top();
}
