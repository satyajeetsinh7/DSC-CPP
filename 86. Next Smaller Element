#include <iostream>
#include <vector>
using namespace std;

vector<int> nextSmallerEle(vector<int>& arr) {
    int n = arr.size();
    
    // initialize all NSEs as -1
    vector<int> result(n, -1); 

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[j] < arr[i]) {
              
                // first smaller element on the right
                result[i] = arr[j]; 
                break;
            }
        }
    }

    return result;
}

int main() {
    vector<int> arr = {4, 8, 5, 2, 25};
    vector<int> nse = nextSmallerEle(arr);

    for (int x : nse) {
        cout << x << " ";
    }
    cout << endl;

    return 0;
}
